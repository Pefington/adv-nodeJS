<%- include('../partials/head') %>

<%- include('../partials/navigation') %>

<main>
  <% if (orders) { %>

  <h2>Orders</h2>

  <% orders.forEach((order) => { %>

  <% order.items.forEach(item => { %>

  <%- include('../partials/product-card.ejs', { order, product: item }) %>

  <% }) %>

  <% })} else { %>

  <h2>No orders found.</h2>

  <% } %>
</main>

<%- include('../partials/end') %>
